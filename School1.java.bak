class School1
{
String Schoolname;
char Schoolgrade;
int Schoolstrength;
School1 (String a,char b,int c)
{
Schoolname=a;
Schoolgrade=b;
Schoolstrength=c;
return;
}
public static void main(String[]args)
{
School1 S1=new School1("jhon",'A',100);
